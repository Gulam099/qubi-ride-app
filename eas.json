{
  "cli": {
    "version": ">= 14.0.2",
    "appVersionSource": "remote"
  },
  "build": {
    "release": {
      "android": {
      "buildType": "apk"
      },
       "ios":{
        "simulator": true
      },
      "channel":"release"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": false,
      "android":{
        "buildType":"apk"
      },
      "ios":{
        "simulator": true
      },
      "channel":"preview"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
