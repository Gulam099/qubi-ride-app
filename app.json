{
  "expo": {
    "name": "Cab Booking",
    "slug": "bla-bla-app",
    "version": "1.0.0",
    "icon": "./assets/logo.jpeg",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "blablaclone",
    "extra": {
      "eas": {
        "projectId": "45b2dd97-0efa-43a8-a6b6-eb55fc861f2d"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/45b2dd97-0efa-43a8-a6b6-eb55fc861f2d"
    },
    "android": {
      "package": "com.halaryde"
    },
    "ios": {
      "bundleIdentifier": "com.halaryde",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
       [
        "expo-splash-screen",
        {
          "image": "./assets/logo.jpeg",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ]
  }
}
